{
  "name": "FB_automation",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "author": "stuz",
  "description": "FB_automation",
  "scripts": {
    "dev": "concurrently \"next dev -p 3000\" \"electron .\"",
    "build:next": "next build",
    "start:next": "next start -p 3000",
    "build": "npm run build:next && npx electron-builder --config electron-builder.yml",
    "start": "electron .",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "next": "15.5.2",
    "prisma": "^6.15.0",
    "puppeteer": "^24.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron": "^38.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
